<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Salary_not_Negative_and_Exceed</fullName>
    <active>true</active>
    <errorConditionFormula>Salary__c &lt; 0 || Salary__c &gt; 80000</errorConditionFormula>
    <errorMessage>Salary cannot be negative and should not exceed 80000 Rs</errorMessage>
</ValidationRule>
